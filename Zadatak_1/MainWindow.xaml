<Window x:Class="Zadatak_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zadatak_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Text="{Binding Print.Text, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="97" Margin="228,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="347"/>
        <ProgressBar HorizontalAlignment="Left" Height="31" VerticalAlignment="Top" Width="347" Margin="228,159,0,0" RenderTransformOrigin="0.029,1.139"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="282,288,0,0" Click="Button_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="444,288,0,0" RenderTransformOrigin="0.234,0.695"/>
        <TextBox Text="{Binding count}" HorizontalAlignment="Left" Height="20" Margin="370,213,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="63"/>

    </Grid>
</Window>
